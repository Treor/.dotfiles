SYSTEM_PATH=$PATH
unset PATH

# keep these on separate lines to make changing their order easier
PATH=$HOME/bin
PATH=$PATH:$HOME/.zsh/bin

if [ -d /usr/local/opt/mysql@5.7 ]; then
  PATH=$PATH:/usr/local/opt/mysql@5.7/bin
fi

PATH=$PATH:/usr/local/bin
PATH=$PATH:/usr/local/sbin
PATH=$PATH:/usr/local/opt/srm/bin
PATH=$PATH:$SYSTEM_PATH
PATH=$PATH:$EC2_HOME/bin
PATH=$PATH:$HOME/.local/bin
PATH=$PATH:$HOME/.cabal/bin
PATH=$PATH:$HOME/.scripts
PATH=$PATH:$HOME/.config/bspwm/panel
PATH=$PATH:$HOME/.gem/ruby/2.6.0/bin
PATH=$PATH:$HOME/.cache/pip
PATH=$PATH:$HOME/.cache/pip/http
PATH=$PATH:$HOME/.cargo/bin
export PATH
